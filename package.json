{
   "name": "ts-cjs-esm",
   "version": "1.0.0",
   "main": "./dist/cjs/index.js",
   "module": "./dist/esm/index.js",
   "types": "./dist/types/index.d.ts",
   "exports": {
      ".": {
         "require": "./dist/cjs/index.js",
         "import": "./dist/esm/index.js"
      }
   },
   "files": [
      "dist"
   ],
   "scripts": {
      "build:cjs": "tsc -p tsconfig.cjs.json",
      "build:esm": "tsc -p tsconfig.esm.json",
      "build": "npm run build:cjs && npm run build:esm && npm run lint && npm run test",
      "clean": "rimraf dist",
      "lint": "eslint .",
      "prepack": "npm run clean && npm run build",
      "format": "prettier --write .",
      "check-format": "prettier --check .",
      "dev": "vitest",
      "test": "vitest run"
   },
   "keywords": [],
   "author": "",
   "license": "MIT",
   "description": "",
   "devDependencies": {
      "@eslint/js": "^9.19.0",
      "eslint": "^9.19.0",
      "globals": "^15.14.0",
      "prettier": "^3.4.2",
      "rimraf": "^5.0.10",
      "tsup": "^8.3.6",
      "typescript": "^5.7.3",
      "typescript-eslint": "^8.22.0",
      "vitest": "^3.0.4"
   },
   "dependencies": {
      "ts-cjs-esm": "file:"
   }
}
